{
  "name": "hpo-tree-visualizer",
  "version": "1.0.0",
  "description": "High-performance web application for exploring the Human Phenotype Ontology (HPO) with interactive tree visualization",
  "main": "hpo_frontend.html",
  "scripts": {
    "start": "python hpo_backend.py",
    "dev": "uvicorn hpo_backend:app --host 0.0.0.0 --port 8000 --reload",
    "serve": "python -m http.server 8080",
    "install-deps": "pip install -r requirements.txt"
  },
  "keywords": [
    "hpo",
    "phenotype",
    "ontology",
    "visualization",
    "tree",
    "fastapi",
    "vis.js",
    "medical",
    "genetics"
  ],
  "author": "HPO Tree Visualizer",
  "license": "MIT",
  "dependencies": {
    "vis-network": "^9.1.6"
  },
  "devDependencies": {},
  "engines": {
    "python": ">=3.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/hpo-tree-visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/hpo-tree-visualizer/issues"
  },
  "homepage": "https://github.com/your-username/hpo-tree-visualizer#readme"
}